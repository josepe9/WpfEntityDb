<UserControl x:Class="WpfEntityDb.Controles.BotonA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfEntityDb.Controles"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Button>
        <StackPanel Orientation="Horizontal" >
            <Image Source="/WpfEntityDb;component/Imagenes/200-add.ico" MinHeight="23" MinWidth="23"/>
            <TextBlock Text="{Binding Texto}" FontSize="30" />
        </StackPanel>
    </Button>
</UserControl>
